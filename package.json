  "version": "1.3.0",
  "description": "Pico.js is a JavaScript library for real-time audio processing that runs a browser and node.js.",
  "author": "nao yonamine <mohayonao@gmail.com>",
  "keywords": [
    "audio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/pico.js.git"
  },
  "main": "pico.node.js",
